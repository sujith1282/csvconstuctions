*{
	margin: 0;
	padding: 0;
	font-family: sans-serif;
}
.heading{
	text-align:center;
}
.name-element{
	display: flex; 
	flex-direction: column;
	align-items: flex-start;
	margin-bottom:10px;
}
.background{
	min-height:500px;
	margin-top:10%;
	border-radius: 15px;
}
.contact{
	display: flex; 
	flex-direction: row;
	justify-content: space-around;
	background-color:#f5d20c10;
}
.reach{
	margin-top:6%;
	background-color:#abffeb20;
	border:1px solid orange;
	border-radius: 15px;
	padding:10px;
	
}
.input{
	width:100%;
	height:22px;
	border:1px solid orange;
	border-radius: 5px;
	
}
.textarea{
	border:1px solid orange;
	border-radius: 5px;
}
	
span{
	color:red;
}
.btn{
	text-align:center;
}
.button{
	background-color:orange;
	border-width:0px;
	border-radius:5px;
	width:60px;
	height:25px;
	margin-top:10%;
	
}


ul.cl{
    list-style-type: none;
	margin: 0;
	padding: 0;
	overflow: hidden;
    color: aliceblue;
    font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
 }
li.cl{
	float: right;
  }
li.cl a{
    display: block;
	color: black;
	text-align: center;
	padding: 15px 17px;
	text-decoration: none;
	}
li.cl a:hover{
	background-color: gold;
	}
    nav {
    position: relative;
    top: 0;
  }
 



.flex_foot{
		width: 33.33%px;
		height: 250px;
		border: 1px solid black;
		display: flex;
	}
.flex_foot div{
		
		flex: 1;
	}
a.social:link,a.social:visited{
	color: white;
	}
a.social:hover{
	color: yellow;
	}
a.social:active{
	color: blue;
	}